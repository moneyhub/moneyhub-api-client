{
  "name": "@mft/moneyhub-api-client",
  "version": "4.15.1",
  "description": "Node.JS client for the Moneyhub API",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/ examples/",
    "test": "mocha --config test/opts/integration.json",
    "test-ci": "mocha --config test/opts/integration-ci.json || true",
